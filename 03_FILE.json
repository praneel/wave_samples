{
  "sfdcExtract": {
    "action": "sfdcDigest",
    "parameters": {
      "fields": [
        {
          "name": "D1__c"
        },
        {
          "name": "LastModifiedDate"
        },
        {
          "name": "CreatedDate"
        },
        {
          "name": "Name"
        }
      ],
      "object": "O1__c"
    }
  },

  "ComExp_today": {
